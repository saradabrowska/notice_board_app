{%  extends 'layout.html.twig' %}

{%  block title %}
    {{ 'title.homepage'|trans }}
{%  endblock %}

{%  block body %}
    <h1>
        {{ 'title.homepage|trans' }}
    </h1>
    {{ form_start(form, { method: 'post', action: url('homepage') }) }}
    {{ form_widget(form) }}
    <input type="submit" value="{{ 'action.findMatchingTypes'|trans }}" class="btn btn-success" />
    {{ form_end(form) }}

{%  endblock %}